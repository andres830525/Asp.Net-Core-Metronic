<!--begin::Navbar-->
<div class="app-navbar flex-shrink-0">
	<!--begin::Search-->
	<div class="d-flex align-items-center align-items-stretch mx-4">
		<partial name="../Search/_Inline.cshtml" />
	</div>
	<!--end::Search-->
	<!--begin::Notifications-->
	<div class="app-navbar-item ms-1 ms-lg-5">
		<!--begin::Menu- wrapper-->
		<div class="btn btn-icon btn-custom btn-color-gray-600 btn-active-light btn-active-color-primary w-35px h-35px w-md-40px h-md-40px" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-attach="parent" data-kt-menu-placement="bottom">@Html.Raw(KTTheme.GetIcon("calendar", "fs-1"))</div>
		<partial name="~/Views/Partials/Menus/_NotificationsMenu.cshtml" />
		<!--end::Menu wrapper-->
	</div>
	<!--end::Notifications-->
	<!--begin::Quick links-->
	<div class="app-navbar-item ms-1 ms-lg-5">
		<!--begin::Menu- wrapper-->
		<div class="btn btn-icon btn-custom btn-color-gray-600 btn-active-light btn-active-color-primary w-35px h-35px w-md-40px h-md-40px" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-attach="parent" data-kt-menu-placement="bottom">@Html.Raw(KTTheme.GetIcon("abstract-26", "fs-1"))</div>
		<partial name="~/Views/Partials/Menus/_QuickLinksMenu.cshtml" />
		<!--end::Menu wrapper-->
	</div>
	<!--end::Quick links-->
	<!--begin::Chat-->
	<div class="app-navbar-item ms-1 ms-lg-5">
		<!--begin::Menu wrapper-->
		<div class="btn btn-icon btn-custom btn-color-gray-600 btn-active-light btn-active-color-primary w-35px h-35px w-md-40px h-md-40px position-relative" id="kt_drawer_chat_toggle">@Html.Raw(KTTheme.GetIcon("notification-on", "fs-1"))</div>
		<!--end::Menu wrapper-->
	</div>
	<!--end::Chat-->
	<!--begin::User menu-->
	<div class="app-navbar-item ms-3 ms-lg-5" id="kt_header_user_menu_toggle">
		<!--begin::Menu wrapper-->
		<div class="cursor-pointer symbol symbol-35px symbol-md-45px" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-attach="parent" data-kt-menu-placement="bottom-end">
			<img class="symbol symbol-circle symbol-35px symbol-md-45px" src='@KTTheme.GetAssetPath("media/avatars/300-13.jpg")' alt="user" />
		</div>
		<partial name="~/Views/Partials/Menus/_UserAccountMenu.cshtml" />
		<!--end::Menu wrapper-->
	</div>
	<!--end::User menu-->
	<!--begin::Header menu toggle-->
	<!--end::Header menu toggle-->
</div>
<!--end::Navbar-->